{
    "presets": [
        ["env", {
            "targets": {
                "browsers": ["chrome >= 60"]
            }
        }]
    ],
    "plugins": [
        "transform-object-assign",
        "transform-class-properties",
        ["transform-runtime", {
            "polyfill": false,
            "regenerator": true
        }]
    ]
}
